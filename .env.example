# cPanel API Configuration
# Copy this file to .env and fill in your actual values

# Your cPanel username
CPANEL_USERNAME=your_cpanel_username

# Your cPanel hostname (e.g., example.com or cpanel.example.com)
CPANEL_HOSTNAME=your_cpanel_hostname

# Your cPanel API token (generate this in cPanel -> Security -> Manage API Tokens)
CPANEL_API_TOKEN=your_api_token_here

# Optional: cPanel port (default is 2083 for SSL, 2082 for non-SSL)
CPANEL_PORT=2083

# Optional: Use SSL connection (true/false, default is true)
CPANEL_SSL=true